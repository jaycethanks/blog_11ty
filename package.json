{
  "name": "11ty-no-style-please",
  "description": "A template for a minimalistic blog",
  "packageManager": "pnpm@8.7.1",
  "scripts": {
    "start": "npx @11ty/eleventy --serve",
    "build": "env NODE_ENV=production npx @11ty/eleventy --pathprefix 'blog_11ty'",
    "i": "pnpm install",
    "clean": "rm -rf _site & pnpm run start"
  },
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-img": "^3.1.8",
    "luxon": "^3.0.1",
    "markdown-it": "^13.0.1"
  },
  "devDependencies": {
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-copy": "^1.2.0",
    "markdown-it-eleventy-img": "^0.10.1",
    "markdown-it-emoji": "^3.0.0",
    "markdown-it-table-of-contents": "^0.6.0"
  }
}
